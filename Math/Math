//A classe Math permite realizar tarefas matemáticas em números como Math.Max que pode ser usado para achar qual o maior valor de duas variáveis

Exemplo:

Console.WriteLine(Math.Max(5, 10));  //Ele vai ver qual o maior valor 5 ou 10 e vai mostrar na tela
Console.WriteLine(Math.Max(20, 3));  //Ele vai ver qual o maior valor 20 ou 3 e vai mostrar na tela
Console.WriteLine(Math.Max(43, 23));  //Ele vai ver qual o maior valor 43 ou 23 e vai mostrar na tela
Console.WriteLine(Math.Max(121, 1023));  //Ele vai ver qual o maior valor 121 ou 1023 e vai mostrar na tela

//Math.Min pode ser usado para encontrar o menor valor o oposto do Math.Max

Exemplo:

Console.WriteLine(Math.Max(23, 10));  //Ele vai ver qual o menor valor 23 ou 10 e vai mostrar na tela
Console.WriteLine(Math.Max(34, 3));  //Ele vai ver qual o menor valor 34 ou 3 e vai mostrar na tela
Console.WriteLine(Math.Max(41, 43));  //Ele vai ver qual o menor valor 41 ou 43 e vai mostrar na tela
Console.WriteLine(Math.Max(98, 53));  //Ele vai ver qual o menor valor 98 ou 53 e vai mostrar na tela

//Math.Sqrt ele retorna a raiz quadrada de um valor

Console.WriteLine(Math.Sqrt(64)); //ELe vai mostrar na tela qual a raiz quadrada de 64
Console.WriteLine(Math.Sqrt(49)); //ELe vai mostrar na tela qual a raiz quadrada de 49
Console.WriteLine(Math.Sqrt(81)); //ELe vai mostrar na tela qual a raiz quadrada de 81
Console.WriteLine(Math.Sqrt(169)); //ELe vai mostrar na tela qual a raiz quadrada de 169

//Math.Abs ele retorna o valor absoluto positivo

Exemplo:

Console.WriteLine(Math.Abs(-4.7));  //Ele vai mostrar na tela o valor absoluto positivo de -4.7
Console.WriteLine(Math.Abs(-43.89));  //Ele vai mostrar na tela o valor absoluto positivo de -43.89
Console.WriteLine(Math.Abs(-33.54));  //Ele vai mostrar na tela o valor absoluto positivo de -33.54
Console.WriteLine(Math.Abs(-98.72));  //Ele vai mostrar na tela o valor absoluto positivo de -98.72

//Math.Round ele arredonda um número para o número inteiro mais próximo

Exemplo:

Console.WriteLine(Math.Round(9.99));  //Ele vai arredondar o valor de 9.99 para um número mais próximo e vai aparecer na tela
Console.WriteLine(Math.Round(12.80));  //Ele vai arredondar o valor de 12.80 para um número mais próximo e vai aparecer na tela
Console.WriteLine(Math.Round(23.73));  //Ele vai arredondar o valor de 23.73 para um número mais próximo e vai aparecer na tela
Console.WriteLine(Math.Round(43.65));  //Ele vai arredondar o valor de 43.65 para um número mais próximo e vai aparecer na tela